
config global
        option anon_swap '0'
        option anon_mount '0'
        option auto_swap '1'
        option auto_mount '1'
        option delay_root '5'
        option check_fs '0'

config mount
        option target '/mnt'
        option enabled '1'
        option device '/dev/sda1'
        option fstype 'vfat'
        option options 'rw,sync,umask=000,iocharset=cp936'
